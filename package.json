    "astro-matomo": "^1.3.0",
    "astro-pagefind": "^1.2.3",
    "astrojs-service-worker": "^1.0.0",
    "floating-vue": "2.0.0-beta.24",
    "graphql": "^16.7.1",
    "graphql-config": "^5.0.2",
    "html-entities": "^2.4.0",
    "lucide-vue-next": "^0.263.0",
    "nanostores": "^0.9.3",
    "prismjs": "^1.29.0",
    "slugify": "^1.6.6"
  }
