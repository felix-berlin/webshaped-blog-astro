{
  "name": "webshaped-blog-frontend",
  "type": "module",
  "version": "1.0.3",
  "description": "My personal blog",
  "homepage": "https://webshaped.de",
  "repository": {
    "type": "git",
    "url": "https://github.com/felix-berlin/webshaped-blog-astro"
  },
  "bugs": {
    "url": "https://github.com/felix-berlin/webshaped-blog-astro/issues"
  },
  "author": {
    "name": "Felix Scholze",
    "email": "felix@webshaped.de",
    "url": "https://webshaped.de"
  },
  "funding": [
    {
      "type": "GitHub Sponsors",
      "url": "https://github.com/sponsors/felix-berlin"
    },
    {
      "type": "Ko-fi",
      "url": "https://ko-fi.com/felixberlin"
    },
    {
      "type": "PayPal",
      "url": "https://paypal.me/FelixScholze"
    },
    {
      "type": "Buy me a coffee",
      "url": "https://www.buymeacoffee.com/felixberlin"
    }
  ],
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "build:server": "export $(cat .env) && astro build",
    "build:strict": "astro check && tsc --noEmit && vue-tsc --noEmit && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "typechecking": "astro check && tsc --noEmit && vue-tsc --noEmit",
    "lint:js": "eslint --ext .ts,.js,.astro,.vue src/ --ignore-path .gitignore --ignore-pattern '/src/tests/*'",
    "lint:style": "stylelint \"**/*.{scss,sass}\" --ignore-path .gitignore",
    "lint": "pnpm run lint:js && pnpm run lint:style",
    "test:unit": "vitest",
    "test:unit:coverage": "vitest run --coverage",
    "test:unit:update": "vitest --update",
    "test:unit:ci": "vitest --coverage --reporter=junit --outputFile=test-report.junit.xml",
    "test:e2e": "npx playwright test",
    "test:e2e:ui": "npx playwright test --ui",
    "format": "npx prettier --write .",
    "gql:generate": "gql-gen --config codegen.ts",
    "predev": "pnpm run gql:generate",
    "preinstall": "npx only-allow pnpm",
    "copy:pagefind": "pnpm dlx shx cp -r dist/pagefind public/",
    "compose:build:up": "docker-compose -f compose.yaml up",
    "compose:build:build": "docker-compose -f compose.yaml build",
    "compose:build:update": "docker-compose -f compose.yaml pull"
  },
  "packageManager": "pnpm@10.8.0",
  "devDependencies": {
    "@astrojs/check": "^0.9.4",
    "@codecov/astro-plugin": "^1.9.0",
    "@faker-js/faker": "^9.6.0",
    "@graphql-codegen/cli": "^5.0.5",
    "@graphql-codegen/client-preset": "^4.8.0",
    "@graphql-codegen/introspection": "^4.0.3",
    "@graphql-codegen/typescript": "^4.1.6",
    "@graphql-codegen/typescript-operations": "^4.6.0",
    "@nanostores/logger": "^0.4.0",
    "@playform/inline": "0.1.2",
    "@playwright/test": "^1.51.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/exec": "^7.0.3",
    "@semantic-release/git": "^10.0.1",
    "@shikijs/transformers": "^3.2.1",
    "@types/he": "^1.2.3",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.14.0",
    "@typescript-eslint/eslint-plugin": "^8.29.1",
    "@typescript-eslint/parser": "^8.29.1",
    "@vite-pwa/astro": "^1.0.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vitest/coverage-v8": "^3.1.1",
    "@vitest/web-worker": "^3.1.1",
    "@vue/compiler-sfc": "^3.5.13",
    "@vue/devtools-api": "^7.7.2",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-vue": "^10.0.0",
    "eslint-plugin-vuejs-accessibility": "^2.4.1",
    "graphql-config": "^5.1.3",
    "jsdom": "^26.0.0",
    "msw": "^2.7.3",
    "postcss-html": "^1.8.0",
    "postcss-preset-env": "^10.1.5",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.5.3",
    "prettier-plugin-astro": "^0.14.1",
    "rollup-plugin-visualizer": "^5.14.0",
    "sass": "^1.86.3",
    "semantic-release": "^24.2.3",
    "sirv": "^3.0.1",
    "stylelint": "^16.18.0",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-recommended-scss": "^14.1.0",
    "stylelint-config-recommended-vue": "^1.6.0",
    "stylelint-config-standard": "^38.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "unplugin-icons": "^22.1.0",
    "vite": "^6.2.5",
    "vitest": "^3.1.1",
    "vue-tsc": "^2.2.8"
  },
  "dependencies": {
    "@astrojs/node": "^9.1.3",
    "@astrojs/rss": "^4.0.11",
    "@astrojs/sitemap": "^3.3.0",
    "@astrojs/vue": "^5.0.9",
    "@felix_berlin/sass-butler": "^3.1.0",
    "@fontsource/source-sans-pro": "^5.2.5",
    "@formkit/auto-animate": "^0.8.2",
    "@iconify-json/lucide": "^1.2.35",
    "@iconify-json/tabler": "^1.2.17",
    "@nanostores/i18n": "^0.12.2",
    "@nanostores/persistent": "^1.0.0",
    "@nanostores/vue": "^0.11.0",
    "@pagefind/default-ui": "^1.3.0",
    "@sentry/astro": "^9.11.0",
    "@spotlightjs/astro": "^3.1.0",
    "@vueuse/core": "^13.0.0",
    "astro": "^5.6.1",
    "astro-breadcrumbs": "^3.3.1",
    "astro-capo": "^0.0.1",
    "astro-matomo": "^1.8.1",
    "floating-vue": "5.2.2",
    "he": "^1.2.0",
    "nanostores": "^0.11.4",
    "pagefind": "^1.3.0",
    "sharp": "^0.34.1",
    "slugify": "^1.6.6",
    "tm-themes": "^1.10.3",
    "vue": "^3.5.13"
  }
}
