    "@iconify-json/lucide": "^1.1.206",
    "@iconify-json/tabler": "^1.1.120",
    "@nanostores/i18n": "^0.12.2",
    "@nanostores/persistent": "^0.10.2",
    "@nanostores/vue": "^0.10.0",
    "@pagefind/default-ui": "^1.1.0",
    "@sentry/astro": "^8.26.0",
    "@vueuse/core": "^10.11.1",
    "astro": "^4.14.0",
    "astro-breadcrumbs": "^3.0.0",
    "astro-matomo": "^1.6.0",
