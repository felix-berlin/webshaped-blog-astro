    "jsdom": "^22.1.0",
    "msw": "^1.3.2",
    "postcss-html": "^1.5.0",
    "postcss-preset-env": "^9.2.0",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.0.3",
