{
  "name": "webshaped-blog-frontend",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "build:strict": "astro check && tsc --noEmit && vue-tsc --noEmit && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "typechecking": "astro check && tsc --noEmit && vue-tsc --noEmit",
    "lint:js": "eslint --ext \".ts,.js,.astro,.vue\" --ignore-path .gitignore",
    "lint:style": "stylelint \"**/*.{scss,sass}\" --ignore-path .gitignore",
    "lint": "pnpm run lint:js && pnpm run lint:style",
    "test:unit": "vitest",
    "test:unit:coverage": "vitest run --coverage",
    "test:e2e": "npx playwright test",
    "test:e2e:ui": "npx playwright test --ui",
    "format": "npx prettier --write .",
    "gql:generate": "gql-gen --config codegen.yml",
    "predev": "pnpm run gql:generate",
    "preinstall": "npx only-allow pnpm"
  },
  "devDependencies": {
    "@astrojs/cloudflare": "^6.8.1",
    "@faker-js/faker": "^8.0.2",
    "@graphql-codegen/cli": "^5.0.0",
