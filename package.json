    "@urql/vue": "^2.0.0",
    "@vueuse/core": "^14.1.0",
    "astro": "^5.16.6",
    "astro-breadcrumbs": "^3.3.3",
    "astro-capo": "^0.0.1",
    "astro-loading-indicator": "^0.7.1",
    "astro-matomo": "^1.9.0",
    "floating-vue": "5.2.2",
    "he": "^1.2.0",
    "nanostores": "^1.1.0",
    "pagefind": "^1.4.0",
